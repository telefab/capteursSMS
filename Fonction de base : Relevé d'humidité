//#include "FreqCounter.h" 
#include "Wire.h" 
#include "HH10D.h"  //HH10D library
 
using namespace std;
 
HH10D humid(6); // Créer l'objet sur la broche voulue
 
void setup()
{
    Wire.begin();
    humid.setup();   // Utilise la librairie Wire pour lire les constantes de l'EEPROM
    humid.setSampleSize(2048); // Détermine le nombre d'échantillons à évaluer pour calculer l'humidité relative moyenne
    Serial.begin(9600);
}
 
void loop()
{
    float humiditeRelative = humid.getRH(); // Calcul de l'humidité relative
    Serial.println(humiditeRelative,2);
    delay(5000);
}
 
